{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "ES2017",
    "module": "Node16",
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "downlevelIteration": true
  },
  "include": ["index.mts"],
  "exclude": ["node_modules"]
}
